{
  "name": "unplugin-swc",
  "version": "0.0.0",
  "description": "SWC plugin for Vite and Rollup",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs --dts",
    "test": "npm run build && uvu -r esbuild-register --ignore fixtures",
    "prepublishOnly": "npm run build"
  },
  "license": "MIT",
  "dependencies": {
    "unplugin": "^0.3.0"
  },
  "devDependencies": {
    "@egoist/prettier-config": "1.0.0",
    "@rollup/pluginutils": "4.2.0",
    "@swc/core": "1.2.160",
    "esbuild": "0.14.9",
    "esbuild-register": "3.3.2",
    "joycon": "3.1.1",
    "jsonc-parser": "3.0.0",
    "path-exists": "5.0.0",
    "prettier": "2.5.1",
    "rollup": "2.70.1",
    "tsup": "5.11.9",
    "typescript": "4.5.4",
    "uvu": "0.5.3",
    "vite": "2.8.6"
  },
  "peerDependencies": {
    "@swc/core": "^1.2.108"
  }
}
